<?xml version="1.0" encoding="utf-8"?>
<ProviderRuleSet xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
                 xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
                 xmlns="http://tempuri.org/MarketDataFormat.xsd">
  <provider>nabCurveDb</provider>
  <rules>
    <dictType>Instrument</dictType>
    <requestType>Undefined</requestType>
    <disabled>false</disabled>
    <priority>2</priority>
    <sourceProvider>nabStandard</sourceProvider>
    <targetProvider>nabCurveDb</targetProvider>
    <sourcePattern>USDJPY-FxCurve</sourcePattern>
    <outputValue>CurveType/String=Forward|LHSAsset/String=JPY|RHSAsset/String=USD</outputValue>
  </rules>
  <rules>
    <dictType>Instrument</dictType>
    <requestType>Undefined</requestType>
    <disabled>false</disabled>
    <priority>2</priority>
    <sourceProvider>nabStandard</sourceProvider>
    <targetProvider>nabCurveDb</targetProvider>
    <sourcePattern>JPYUSD-FxForward-([0-9a-z]+)</sourcePattern>
    <outputValue>CurveType/String=Forward|LHSAsset/String=USD|RHSAsset/String=JPY|Tenor/String=$1</outputValue>
  </rules>
  <rules>
    <dictType>FieldName</dictType>
    <requestType>Undefined</requestType>
    <disabled>false</disabled>
    <priority>2</priority>
    <sourceProvider>nabStandard</sourceProvider>
    <targetProvider>nabCurveDb</targetProvider>
    <sourcePattern>Last</sourcePattern>
    <outputValue>Rate</outputValue>
  </rules>
  <rules>
    <dictType>Instrument</dictType>
    <requestType>Undefined</requestType>
    <disabled>false</disabled>
    <priority>2</priority>
    <sourceProvider>nabStandard</sourceProvider>
    <targetProvider>nabCurveDb</targetProvider>
    <sourcePattern>USDJPY-FxForward-([0-9a-z]+)</sourcePattern>
    <outputValue>CurveType/String=Forward|LHSAsset/String=JPY|RHSAsset/String=USD|Tenor/String=$1</outputValue>
  </rules>
  <rules>
    <dictType>Instrument</dictType>
    <requestType>Undefined</requestType>
    <disabled>false</disabled>
    <priority>2</priority>
    <sourceProvider>nabStandard</sourceProvider>
    <targetProvider>nabCurveDb</targetProvider>
    <sourcePattern>JPYUSD-FxCurve</sourcePattern>
    <outputValue>CurveType/String=Forward|LHSAsset/String=USD|RHSAsset/String=JPY</outputValue>
  </rules>
  <rules>
    <dictType>Instrument</dictType>
    <requestType>Undefined</requestType>
    <disabled>false</disabled>
    <priority>1</priority>
    <sourceProvider>nabStandard</sourceProvider>
    <targetProvider>nabCurveDb</targetProvider>
    <sourcePattern>([a-z][a-z][a-z])-RateCurve</sourcePattern>
    <outputValue>CurveType/String=Zero|LHSAsset/String=$1|RHSAsset/String=$1</outputValue>
  </rules>
  <rules>
    <dictType>Instrument</dictType>
    <requestType>Undefined</requestType>
    <disabled>false</disabled>
    <priority>1</priority>
    <sourceProvider>nabStandard</sourceProvider>
    <targetProvider>nabCurveDb</targetProvider>
    <sourcePattern>([a-z][a-z][a-z])-Deposit-([0-9a-z]*)</sourcePattern>
    <outputValue>CurveType/String=Zero|LHSAsset/String=$1|RHSAsset/String=$1|Tenor/String=$2</outputValue>
  </rules>
  <rules>
    <dictType>QuoteUnits</dictType>
    <requestType>Undefined</requestType>
    <disabled>false</disabled>
    <priority>1</priority>
    <sourceProvider>nabStandard</sourceProvider>
    <targetProvider>nabCurveDb</targetProvider>
    <sourcePattern>(.*)-IRFuture-(.*)/(.*)</sourcePattern>
    <outputValue>IRFuturesPrice</outputValue>
  </rules>
  <rules>
    <dictType>Instrument</dictType>
    <requestType>Undefined</requestType>
    <disabled>false</disabled>
    <priority>1</priority>
    <sourceProvider>nabStandard</sourceProvider>
    <targetProvider>nabCurveDb</targetProvider>
    <sourcePattern>([a-z][a-z][a-z])([a-z][a-z][a-z])-FxCurve</sourcePattern>
    <outputValue>CurveType/String=Forward|LHSAsset/String=$1|RHSAsset/String=$2</outputValue>
  </rules>
  <rules>
    <dictType>Instrument</dictType>
    <requestType>Undefined</requestType>
    <disabled>false</disabled>
    <priority>1</priority>
    <sourceProvider>nabStandard</sourceProvider>
    <targetProvider>nabCurveDb</targetProvider>
    <sourcePattern>([a-z][a-z][a-z])([a-z][a-z][a-z])-FxForward-([0-9a-z]+)</sourcePattern>
    <outputValue>CurveType/String=Forward|LHSAsset/String=$1|RHSAsset/String=$2|Tenor/String=$3</outputValue>
  </rules>
  <rules>
    <dictType>QuoteUnits</dictType>
    <requestType>Undefined</requestType>
    <disabled>false</disabled>
    <priority>0</priority>
    <sourceProvider>nabStandard</sourceProvider>
    <targetProvider>nabCurveDb</targetProvider>
    <sourcePattern>(.*)/(.*)</sourcePattern>
    <outputValue>Rate</outputValue>
  </rules>
