<?xml version="1.0" encoding="UTF-8"?>
<FpML xmlns="http://www.fpml.org/2009/FpML-4-7" 
	xmlns:ext="http://www.fpml.org/2009/FpML-4-7/ext" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://www.fpml.org/2009/FpML-4-7 ../FpML-4-7-8-TR-1/fpml-main-4-7.xsd http://www.fpml.org/2009/FpML-4-7/ext ../fpmlext-main.xsd" 
	xsi:type="RequestTradeConfirmation" 
	version="4-7">
	
	<header>
		<messageId 
			messageIdScheme="http://www.jpmorgan.com/coding-scheme/message-id">345345</messageId>
		<sentBy 
			partyIdScheme="http://www.jpmorgan.com/coding-scheme/party-id">bmf</sentBy>
		<creationTimestamp>2000-08-01T08:57:00-00:00</creationTimestamp>
	</header>
	
	<trade>
		<tradeHeader>
			<partyTradeIdentifier>
				<partyReference href="jb2890"/>
				<tradeId 
					tradeIdScheme="http://www.PartyA.com/coding-scheme/trade-id">1234</tradeId>
			</partyTradeIdentifier>
			<tradeDate>2005-07-13</tradeDate>
		</tradeHeader>
		
		<ext:swapTransaction>
			
			<!-- mandatory reference to buyer -->
			<buyerPartyReference href="hwy190"/>
			<!-- mandatory reference to seller -->
			<sellerPartyReference href="klq902"/>
			
			<!-- mandatory notional amount USD 5 million -->
			<ext:notionalAmount>
				<currency>USD</currency>
				<amount>5000000</amount>
			</ext:notionalAmount>
			
			<!-- mandatory price 6.5 % -->
			<ext:price>0.0650</ext:price>
			
			<!-- mandatory identification of swap -->
			<ext:swap>
				<!-- CME identifier for this swap -->
				<instrumentId 
					instrumentIdScheme="http://www.cme.com/coding-scheme/instrument-id">82992</instrumentId>
				<!-- optional currency -->
				<currency>USD</currency>
				<!-- optional exchange identifier -->
				<exchangeId>CME</exchangeId>
				<!-- mandatory floating rate index -->
				<floatingRateIndex>USD-LIBOR</floatingRateIndex>
				<!-- mandtory term -->
				<term>
					<periodMultiplier>1</periodMultiplier>
					<period>Y</period>
				</term>
			</ext:swap>
			
		</ext:swapTransaction>

	</trade>
	
	<party id="jb2890">
		<partyId>Party A</partyId>
	</party>
	<party id="klq902">
		<partyId>klq902</partyId>
	</party>
	<party id="hwy190">
		<partyId>LWT</partyId>
	</party>
</FpML>
