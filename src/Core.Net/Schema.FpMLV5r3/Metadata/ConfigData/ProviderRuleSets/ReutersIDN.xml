<?xml version="1.0" encoding="utf-8"?>
<ProviderRuleSet xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
                 xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
                 xmlns="http://tempuri.org/MarketDataFormat.xsd">
  <provider>ReutersIDN</provider>
  <rules>
    <dictType>FieldName</dictType>
    <requestType>Undefined</requestType>
    <disabled>false</disabled>
    <priority>2</priority>
    <sourceProvider>GlobalIB</sourceProvider>
    <targetProvider>ReutersIDN</targetProvider>
    <sourcePattern>Bid-Live</sourcePattern>
    <outputValue>BID</outputValue>
  </rules>
  <rules>
    <dictType>FieldName</dictType>
    <requestType>Undefined</requestType>
    <disabled>false</disabled>
    <priority>2</priority>
    <sourceProvider>GlobalIB</sourceProvider>
    <targetProvider>ReutersIDN</targetProvider>
    <sourcePattern>Ask-Live</sourcePattern>
    <outputValue>ASK</outputValue>
  </rules>
  <rules>
    <dictType>Instrument</dictType>
    <requestType>Undefined</requestType>
    <disabled>false</disabled>
    <priority>2</priority>
    <sourceProvider>GlobalIB</sourceProvider>
    <targetProvider>ReutersIDN</targetProvider>
    <sourcePattern>([a-z][a-z][a-z])-FxSpot-SP</sourcePattern>
    <outputValue>$1=</outputValue>
  </rules>
  <rules>
    <dictType>Instrument</dictType>
    <requestType>Undefined</requestType>
    <disabled>false</disabled>
    <priority>2</priority>
    <sourceProvider>GlobalIB</sourceProvider>
    <targetProvider>ReutersIDN</targetProvider>
    <sourcePattern>([a-z][a-z][a-z])USD-FxSpot-SP</sourcePattern>
    <outputValue>$1=</outputValue>
  </rules>
  <rules>
    <dictType>QuoteUnits</dictType>
    <requestType>Undefined</requestType>
    <disabled>false</disabled>
    <priority>1</priority>
    <sourceProvider>GlobalIB</sourceProvider>
    <targetProvider>ReutersIDN</targetProvider>
    <sourcePattern>(.*)-IRFuture-(.*)/(.*)</sourcePattern>
    <outputValue>IRFuturesPrice</outputValue>
  </rules>
  <rules>
    <dictType>Instrument</dictType>
    <requestType>Undefined</requestType>
    <disabled>false</disabled>
    <priority>1</priority>
    <sourceProvider>GlobalIB</sourceProvider>
    <targetProvider>ReutersIDN</targetProvider>
    <sourcePattern>([a-z][a-z][a-z])([a-z][a-z][a-z])-FxSpot-SP</sourcePattern>
    <outputValue>$1$2=</outputValue>
  </rules>
  <rules>
    <dictType>QuoteUnits</dictType>
    <requestType>Undefined</requestType>
    <disabled>false</disabled>
    <priority>0</priority>
    <sourceProvider>GlobalIB</sourceProvider>
    <targetProvider>ReutersIDN</targetProvider>
    <sourcePattern>(.*)/(.*)</sourcePattern>
    <outputValue>Rate</outputValue>
  </rules>
</ProviderRuleSet>
