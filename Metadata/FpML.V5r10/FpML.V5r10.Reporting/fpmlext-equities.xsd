<xsd:schema xmlns="http://www.fpml.org/FpML-5/ext" xmlns:fpml="http://www.fpml.org/FpML-5/reporting" xmlns:dsig="http://www.w3.org/2000/09/xmldsig#" targetNamespace="http://www.fpml.org/FpML-5/ext" xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	
	<xsd:include schemaLocation="fpmlext-shared.xsd"/>
	
	<xsd:import namespace="http://www.fpml.org/FpML-5/reporting" schemaLocation="fpml-main-5-10.xsd"/>

	<xsd:complexType name="EquityTransaction">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Equity Transaction.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="AbstractUnitTransaction">
				<xsd:sequence>
					<xsd:element name="equity" type="fpml:EquityAsset">
						<xsd:annotation>
							<xsd:documentation xml:lang="en">Identification of a Equity.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
	<xsd:element name="equityTransaction" type="EquityTransaction" substitutionGroup="fpml:product">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Global element representing an Equity Transaction.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
</xsd:schema>